{
    "name": "bracket-select-plus",
    "displayName": "Bracket Select",
    "description": "Quick select code between brackets, support for (), {}, [], '' and \"\"",
    "version": "0.9.9",
    "publisher": "pit00",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.34.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pit00/vscode-bracket-select-plus.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "extensionKind":[
        "ui"
    ],
    "main": "./out/src/bracketSelectMain",
    "contributes": {
        "commands": [
            {
                "command": "bracket-select-plus.select",
                "title": "Brackets select"
            },
            {
                "command": "bracket-select-plus.select-include",
                "title": "Brackets select include"
            }
        ],
        "keybindings": [
            {
                "command": "bracket-select-plus.select",
                "key": "alt+a",
                "mac": "alt+a",
                "when": "editorTextFocus"
            },
            {
                "command": "bracket-select-plus.select-include",
                "key": "ctrl+alt+a",
                "mac": "cmd+alt+a",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.34.0",
        "tslint": "^5.19.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {}
}
